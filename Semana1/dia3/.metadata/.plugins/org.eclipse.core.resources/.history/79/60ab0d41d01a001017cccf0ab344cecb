		// TODO Auto-generated method stub
		
	}

	@Override
	public void eliminar(Empleado empleado) {
		// TODO Auto-generated method stub
		
	}

	@Override
	public void mostrar() {
		// TODO Auto-generated method stub
		
	}

	
	
